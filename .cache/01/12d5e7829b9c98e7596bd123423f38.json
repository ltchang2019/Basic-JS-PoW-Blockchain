"components/TransactionPool.js","original":{"line":67,"column":16}},{"generated":{"line":127,"column":29},"source":"components/TransactionPool.js","original":{"line":53,"column":12}},{"generated":{"line":127,"column":30},"source":"components/TransactionPool.js","original":{"line":52,"column":8}},{"generated":{"line":128,"column":0},"source":"components/TransactionPool.js","original":{"line":70,"column":5}},{"name":"Component","generated":{"line":132,"column":2},"source":"components/TransactionPool.js","original":{"line":18,"column":30}},{"name":"TransactionPool","generated":{"line":134,"column":15},"source":"components/TransactionPool.js","original":{"line":73,"column":15}}],"sources":{"components/TransactionPool.js":"import React, { Component } from 'react';\nimport { Link } from 'react-router-dom';\nimport Transaction from './Transaction';\nimport { Button } from 'react-bootstrap';\nimport history from '../history';\n\nconst POLL_INTERVAL = 10000;\n\n/* Component: TransactionPool\n * ______________________________\n * Summary:\n *  - shows transaction pool and has button to mine transactions\n *  - gets transaction pool through fetch request to api\n *  - maps through values in transaction pool to display all transactions\n *  - sets interval to request new transaction pool data every 10 seconds\n *  - mine transaction button makes api call and redirects to blocks page if successful\n */\nclass TransactionPool extends Component {\n    state = { transactionPoolMap: {} };\n\n    getTransactionPoolMap = () => {\n        fetch(`${document.location.origin}/api/get-transact-pool-map`)\n            .then(response => response.json())\n            .then(json => this.setState({ transactionPoolMap: json }));\n    }\n\n    mineTransactions = () => {\n        fetch(`${document.location.origin}/api/mine-transactions`)\n            .then(response => {\n                if(response.status === 200) {\n                    alert('success');\n                    history.push('/blocks');\n                } else {\n                    alert('Mine request did not complete.');\n                }\n            });\n    }\n\n    componentDidMount() {\n        this.getTransactionPoolMap();\n\n        this.getPoolInterval = setInterval(\n            () => this.getTransactionPoolMap(), POLL_INTERVAL\n        );\n    }\n\n    componentWillUnmount() {\n        clearInterval(this.getPoolInterval);\n    }\n\n    render() {\n        return (\n            <div className='TransactionPool'>\n                <div><Link to='/'>Home</Link></div>\n                <h3>Transaction Pool</h3>\n                {\n                    Object.values(this.state.transactionPoolMap).map(transaction => {\n                        return (\n                            <div key={transaction.id}>\n                                <hr />\n                                <Transaction className='Transaction' transaction={transaction} />\n                            </div>\n                        )\n                    })\n                }\n                <hr />\n                <Button onClick={this.mineTransactions}>Mine Transactions</Button>\n            </div>\n        );\n    }\n}\n\nexport default TransactionPool;"},"lineCount":null}},"error":null,"hash":"6bfdb8edf4f8860a7ce95869ddf71c3d","cacheData":{"env":{}}}